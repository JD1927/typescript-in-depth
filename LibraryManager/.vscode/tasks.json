{
  "version": "2.0.0",
  "command": "tsc",
  "shell": true,
  "presentation": {
    "echo": true,
    "reveal": "silent",
    "focus": false,
    "panel": "shared",
    "showReuseMessage": true,
    "clear": false
  },
  "args": ["--target","ES5",
    "--outDir","js",
    "--module","commonjs",
    "--sourceMap",
    "--watch",
    "app.ts"],
  "problemMatcher": "$tsc"
}